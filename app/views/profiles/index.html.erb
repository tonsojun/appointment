<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table class='table'>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Birth</th>
      <th>Marital status</th>
      <th>Home phone</th>
      <th>Cell phone</th>
      <th>Gender</th>
      <th>License</th>
      <th>Drivers license</th>
      <th>Specialty</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.first_name %></td>
        <td><%= profile.middle_name %></td>
        <td><%= profile.last_name %></td>
        <td><%= profile.birth %></td>
        <td><%= profile.marital_status %></td>
        <td><%= profile.home_phone %></td>
        <td><%= profile.cell_phone %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.license %></td>
        <td><%= profile.drivers_license %></td>
        <td><%= profile.specialty_id %></td>
        <td><%= profile.user_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
